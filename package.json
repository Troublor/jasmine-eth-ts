{
  "name": "jasmine-eth-ts",
  "version": "1.0.18",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/Troublor/jasmine-eth-ts.git",
  "author": "Troublor",
  "license": "Apache-2.0",
  "scripts": {
    "test": "mocha --require ts-node/register tests/**/*.test.ts",
    "build": "tsc && cp src/contracts/* dist/src/contracts",
    "build:docs": "typedoc --out docs --exclude node_modules --exclude tests --excludeNotExported --excludePrivate --excludeProtected --mode modules index.ts",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --fix",
    "prepare": "yarn lint && yarn build && yarn test && yarn build:docs"
  },
  "dependencies": {
    "bn.js": "^5.1.3",
    "ethereumjs-tx": "^2.1.2",
    "ganache-core": "^2.13.1",
    "root-require": "^0.3.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "web3": "^1.3.0"
  },
  "devDependencies": {
    "@typechain/web3-v1": "^1.0.0",
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.3",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "chai": "^4.2.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "ethereumjs-wallet": "^1.0.1",
    "ganache-cli": "^6.10.2",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "typechain": "^2.0.0",
    "typedoc": "^0.17.0-3"
  }
}
